                                                    <AppBarButton.Icon>
                                                        <FontIcon Glyph="&#xE9DD;" FontFamily="{StaticResource Icon}"/>
                                                    </AppBarButton.Icon>
                                                </AppBarButton>
                                                <AppBarButton x:Name='PreviousTrackButton'
