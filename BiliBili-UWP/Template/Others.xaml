                                                        <FontIcon FontFamily="{StaticResource Icon}" Glyph="&#xE99A;"/>
                                                    </AppBarButton.Icon>
                                                </AppBarButton>
                                            </CommandBar.PrimaryCommands>
                                        </CommandBar>
                                    </Border>
                                </Grid>
                            </Grid>

                        </Border>

                        <Border x:Name="ErrorBorder"></Border>
                        <Border  VerticalAlignment="Bottom" Margin="0 0 0 82" HorizontalAlignment="Left">
