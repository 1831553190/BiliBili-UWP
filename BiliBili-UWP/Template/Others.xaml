                                                <FontIcon x:Name="PlayPauseSymbolLeft" FontFamily="{StaticResource Icon}" Glyph="&#xE9BE;" />
                                            </AppBarButton.Icon>
                                        </AppBarButton>
                                    </Border>
                                    <Border x:Name="MediaTransportControls_Command_Border"
                                    Grid.Column="1"
                                    Grid.Row="2">
