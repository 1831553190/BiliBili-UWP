                                                    <AppBarButton.Icon>
                                                        <FontIcon Glyph="&#xE9B2;" FontFamily="{StaticResource Icon}"/>
                                                    </AppBarButton.Icon>
                                                </AppBarButton>
                                                <AppBarButton x:Name='SkipBackwardButton'
